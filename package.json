{
  "name": "@scintilla-network/hashes",
  "version": "1.2.0",
  "description": "Enhanced hash functions for Scintilla and broader crypto / blockchain use",
  "type": "module",
  "main": "src/index.js",
  "types": "src/index.d.ts",
  "exports": {
    ".": {
      "types": "./src/index.d.ts",
      "default": "./src/index.js"
    },
    "./classic": {
      "types": "./src/classic/index.d.ts",
      "default": "./src/classic/index.js"
    },
    "./pq": {
      "types": "./src/pq/index.d.ts",
      "default": "./src/pq/index.js"
    },
    "./utils": {
      "types": "./src/utils/index.d.ts",
      "default": "./src/utils/index.js"
    }
  },
  "files": [
    "src",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "test": "litest",
    "typecheck": "tsc",
    "prepublishOnly": "npm run test"
  },
  "keywords": [
    "crypto",
    "hash",
    "sha3",
    "blake3",
    "post-quantum",
    "scintilla",
    "bech32",
    "bech32m",
    "hash160",
    "ripemd160",
    "sha256",
    "sha512"
  ],
  "author": "Scintilla Labs",
  "license": "MIT",
  "dependencies": {
    "@noble/hashes": "2.0.0"
  },
  "devDependencies": {
    "@scintilla-network/litest": "1.3.0"
  }
}
